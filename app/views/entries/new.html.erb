<%= form_with model: @entry, url: entries_path, data: { turbo: false } do |form| %>

<div class="mb-3">
    <%= form.label :title %>
    <%= form.text_field :title, class: "form-control" %>
</div>
<div class="mb-3">
    <%= form.label :content %>
    <%= form.text_field :content, class: "form-control" %>
</div>
<div>
    <%= form.label :image %>
    <%= form.file_field :image %>
</div>
<div class="mb-3">
    <%= form.submit "Submit Entry", class: "btn btn-primary" %>
</div>

<% end %>